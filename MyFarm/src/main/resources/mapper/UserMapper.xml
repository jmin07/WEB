<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="farmstory.myfarm.domain.user.repository.UserRepository">

    <select id="selectUser">
        select * from 테이블 명 where email = #{email}
    </select>

    <insert id="createUser">
        insert into 테이블 (컬럼명) values(값들)
    </insert>

    <update id="updateUser">
        update 테이블 명 set 컬럼 = 변경 값 where email = #{uservo.email};
    </update>

    <delete id="deleteUser">
        delete from 테이블명 where email = #{uservo.email}
    </delete>
</mapper>